#!/bin/sh
set -e
if [ -n "$D" -o ! -d /run/systemd/system ] && [ -z "$D" -a -x "/etc/init.d/run-postinsts" ]; then
	/etc/init.d/run-postinsts stop || :
fi
if systemctl >/dev/null 2>/dev/null; then
	if [ -z "$D" ]; then
		systemctl stop run-postinsts.service

		systemctl disable run-postinsts.service
	fi
fi
